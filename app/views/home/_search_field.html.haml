.col.md-12.frame-form
  %table.table.table-condensed
    %thead
    %tr
      %td
        = search_form_for @q, html: {class: "form-tag"} do |f|
          .input-group
            = f.search_field :chemical_name_cont, placeholder: 'Enter name of searched compound', class: "form-control"
            %span.input-group-btn
              = f.submit "Search", class: 'btn btn-secondary'
        = search_form_for @q, html: {class: "form-tag"} do |f|
          .input-group
            = f.search_field :cas_number_cont, placeholder: 'Enter CAS of searched compound', class: "form-control"
            %span.input-group-btn
              = f.submit "Search", class: 'btn btn-secondary'


