<p id="notice"><%= notice %></p>

<h1>Safety Precautions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @safety_precautions.each do |safety_precaution| %>
      <tr>
        <td><%= link_to 'Show', safety_precaution %></td>
        <td><%= link_to 'Edit', edit_safety_precaution_path(safety_precaution) %></td>
        <td><%= link_to 'Destroy', safety_precaution, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

